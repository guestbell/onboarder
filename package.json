{
  "name": "@guestbell/onboarder",
  "version": "1.0.5",
  "description": "Nonlinear user onboarding done in react. Think state machine forms - steps in any order",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs -s ./src/stories/assets",
    "build": "rollup -c",
    "version:patch": "npm version patch",
    "push:git": "git push --tags origin main --quiet",
    "commit:build": "git add . && git commit -m \"Deploy commit\" --quiet && npm run push:git || true",
    "create:patch": "npm run version:patch && npm run push:git",
    "deploy": "npm run build && npm run build-storybook && npm run commit:build && npm publish && npm run create:patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestbell/onboarder.git"
  },
  "keywords": [
    "react",
    "onboarding",
    "wizard"
  ],
  "author": "Peter Kottas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guestbell/onboarder/issues"
  },
  "homepage": "https://guestbell.github.io/onboarder/",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^8.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.11.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
